  },
  "scripts": {
    "test": "cross-env TEST_REPORT_PATH=reports jest --coverage",
    "api": "json-server -w src/mocks/api-mock-response.json ---port 3004",
    "test:watch": "cross-env TEST_REPORT_PATH=reports npm test -- --watch",
    "build": "webpack --progress",
    "build:prod": "cross-env NODE_ENV=production webpack --profile -p",
    "start": "webpack-dev-server --hot --inline --port 9090 --content-base public/",
    "start:open": "webpack-dev-server --hot --inline --port 9090 --open --content-base public/",
    "serve:prod": "webpack-dev-server --port 9090 --content-base build/"
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "<rootDir>/(build|docs|node_modules)/",
      "<rootDir>/src/(constants|utils)"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleNameMapper": {
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/mocks/FileStub.js",
      "^.+\\.s?.css$": "<rootDir>/src/mocks/CSSStub.js"
    },
    "setupTestFrameworkScriptFile": "<rootDir>/src/mocks/setupTests.js",
    "testPathDirs": [
      "<rootDir>/src/"
    ],
    "testResultsProcessor": "jest-sonar-reporter"
  }
}
